doctype html
html
  head
    title View Paste
    link(rel='stylesheet', href='resources/navbar.css')
    link(rel='stylesheet', href='resources/view.css')
  body
    ul.navbar
      li#logo
        a(href='#Logo')
          i
      li#maintext
        a(href='#home') PASTEBIN
      li
        a(href='/') Home
      li
        a(href='#about') About
    #syntax.styled_text
      p View Paste:
    #editor #{paste}
    #syntax.styled_text
    .options
      p.optionstext(style='display:inline-block;margin-right:10px; font-size:125%;') Options: 
      button.optionbutton View Raw
      button.optionbutton Clone
  script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js')
  script(src='resources/ace-builds/src-noconflict/ace.js', type='text/javascript', charset='utf-8')
  script.
    var editor = ace.edit("editor");
    editor.setTheme("ace/theme/sqlserver");
    editor.setOptions({
        maxLines: Infinity,
        minLines: 25
    });
    editor.setReadOnly(true);
