doctype html
html
  head
    title Pastebin
    link(rel='stylesheet', href='resources/navbar.css')
    link(rel='stylesheet', href='resources/main.css')
  body
    ul.navbar
      li#logo
        a(href='/')
          i
      li#maintext
        a(href='/') PASTEBIN
      li
        a.active(href='/') Home
      li
        a(href="/about") About
    #newpaste
      p New Paste:
    #editor #{clonepaste}
    .syntax-highlight
      p(style='display:inline-block;margin-right:10px; font-size:125%;')  Syntax Highlighting: 
      select#syntax-highlight-selector
        option(value='none') None
        option(value='cpp') C
        option(value='cpp') C++
        option(value='csharp') C#
        option(value='golang') Golang
        option(value='java') Java
        option(value='lua') Lua
        option(value='mysql') MySQL
        option(value='php') PHP
        option(value='python') Python
    button#submitbutton Create New Paste
  script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js')
  script(src='resources/ace-builds/src-noconflict/ace.js', type='text/javascript', charset='utf-8')
  script(src='resources/main.js')